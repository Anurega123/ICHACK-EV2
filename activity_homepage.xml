<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bgm2"
    tools:context=".homepage">


    <ImageView
        android:layout_width="74dp"
        android:layout_height="76dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="-4dp"
        android:background="@drawable/ev3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"></ImageView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="108dp"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/skranji"
        android:text="Emergency vehicle vigilant"
        android:textColor="@color/white"
        android:textSize="22dp"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:layout_marginTop="116dp"
        android:fontFamily="@font/skranji"
        android:padding="10dp"
        android:text="EMGERNCY RESCUE"
        android:textColor="@color/black"
        android:textSize="32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </TextView>

    <LinearLayout
        android:id="@+id/soss"
        android:layout_width="170dp"
        android:layout_height="170dp"
        android:layout_marginStart="120dp"
        android:layout_marginTop="216dp"
        android:background="@drawable/source"
        android:elevation="20dp"
        android:gravity="center"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:layout_width="74dp"
            android:layout_height="70dp"
            android:layout_gravity="center"
            android:layout_marginLeft="0dp"
            android:fontFamily="@font/skranji"
            android:gravity="center"
            android:text="SOS"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="32dp"
            android:textStyle="bold"></TextView>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/sss"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="140dp"
        android:layout_marginTop="424dp"
        android:background="@drawable/source"
        android:elevation="20dp"
        android:gravity="center"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:background="@drawable/bunk1"></ImageView>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/sss2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="424dp"
        android:background="@drawable/source"
        android:elevation="20dp"
        android:gravity="center"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:background="@drawable/mech1"></ImageView>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/sss3"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="272dp"
        android:layout_marginTop="424dp"
        android:background="@drawable/source"
        android:elevation="20dp"
        android:gravity="center"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:background="@drawable/evc"></ImageView>
    </LinearLayout>

    <LinearLayout
        android:layout_width="350dp"
        android:layout_height="70dp"
        android:layout_marginStart="28dp"
        android:layout_marginBottom="28dp"
        android:background="@drawable/borderinpreg"
        android:elevation="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent">
        <ImageView
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:background="@drawable/logout"
            android:layout_marginTop="15dp"
            android:id="@+id/logouth"
            android:layout_marginLeft="50dp">
        </ImageView>
        <ImageView
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:background="@drawable/account"
            android:layout_marginTop="15dp"
            android:layout_marginLeft="60dp">
        </ImageView>
        <ImageView
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:background="@drawable/location"
            android:layout_marginTop="15dp"
            android:id="@+id/getloactionh"
            android:layout_marginLeft="60dp">
        </ImageView>


    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>